<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd
        http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.2.xsd">
    
    <bean id="roleEntityProvider" class="pl.exsio.frameset.vaadin.entity.provider.security.RoleEntityProvider">
    </bean>
    
    <bean id="groupEntityProvider" class="pl.exsio.frameset.vaadin.entity.provider.security.GroupEntityProvider">
    </bean>
    
    <bean id="userEntityProvider" class="pl.exsio.frameset.vaadin.entity.provider.security.UserEntityProvider">
    </bean>
    
    <bean id="frameEntityProvider" class="pl.exsio.frameset.vaadin.entity.provider.generic.FrameEntityProvider">
    </bean>
    
</beans>